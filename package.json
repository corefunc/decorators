    "dist/index.cjs.map",
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.js.map",
    "dist/index.mjs",
    "dist/index.mjs.map"
  ],
  "homepage": "https://github.com/corefunc/decorators",
  "keywords": [
    "aop",
    "aspects",
    "decorator",
    "decorators",
    "mixin"
