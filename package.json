    "babel-eslint": "10.1.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jsdoc": "24.0.6",
    "eslint-plugin-prettier": "3.1.3",
    "jest": "26.0.1",
    "npm-check": "5.9.2",
    "prettier": "2.0.5"
  },
  "engines": {
    "node": ">=12.9.0"
  },
  "engineStrict": true,
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
